{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "quoteProps": "consistent",
  "proseWrap": "never",
  "jsxSingleQuote": false,
  "htmlWhitespaceSensitivity": "strict",
  "endOfLine": "lf"
}
